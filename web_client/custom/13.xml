<component width="300" height="300">
	<webparser url="http://kyosu.mooo.com/php/checkbroadcast.php?onair=1" regex="(?=&lt;struct&gt;).*&lt;struct&gt;.*&lt;string&gt;(.*)&lt;\/string&gt;.*&lt;string&gt;(.*)&lt;\/string&gt;.*&lt;string&gt;(.*)&lt;\/string&gt;.*&lt;string&gt;(.*)&lt;\/string&gt;.*&lt;string&gt;(.*)&lt;\/string&gt;.*&lt;string&gt;(.*)&lt;\/string&gt;.*&lt;\/struct&gt;">
