<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/popup_dash"
    android:layout_width="200dp"
    android:layout_height="fill_parent"
    android:orientation="vertical"
	android:gravity="center"
	>

    <Button
        android:id="@+id/dashoption_correction"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/login_edittext_background"
        android:text="Dashboard 이름 변경" />

    <Button
        android:id="@+id/dashoption_bookmark"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/login_edittext_background"
        android:text="Dashboard 북마크에 추가" />
    
    <Button
        android:id="@+id/dashoption_delete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/login_edittext_background"
        android:text="Dashboard 삭제" />

</LinearLayout>
